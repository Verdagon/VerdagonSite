{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/home",
        "type": 302
      },
      {
        "source": "/blog/next-decade-languages-gpu",
        "destination": "/blog/next-gen-languages-gpu",
        "type": 302
      },
      {
        "source": "/blog/next-fearless-ffi",
        "destination": "/blog/next-gen-languages-gpu",
        "type": 302
      },
      {
        "regex": "/guide/(.+)",
        "destination": "https://vale.dev/guide/:1",
        "type": 302
      },
      {
        "regex": "/vision/(.+)",
        "destination": "https://vale.dev/vision/:1",
        "type": 302
      },
      {
        "source": "/blog/0.2/on-removing-let-let-mut",
        "destination": "/blog/on-removing-let-let-mut",
        "type": 302
      },
      {
        "source": "/blog/probabilistic-memory-safety",
        "destination": "/blog/thoughts-on-perfect-memory-safety",
        "type": 302
      }
    ]
  }
}
